numbers = []

for i in range(5):
    num = int(input(f"Enter number {i+1}: "))
    numbers.append(num)

total = sum(numbers)
maximum = max(numbers)
minimum = min(numbers)
average = total / len(numbers)

print("\nSum =", total)
print("Max =", maximum)
print("Min =", minimum)
print("Average =", average)
